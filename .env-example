# Server configs
BACKLOG=2048
BLOCKING_WORKER_POOL_SIZE=512 # per worker
CLIENT_DISCONNECT_TIMEOUT_MS=5000
CLIENT_REQ_TIMEOUT_MS=5000
HOST=127.0.0.1
KEEP_ALIVE_S=5
MAX_CONNECTIONS=25000 # per worker
TLS_RATE=256 # per worker
