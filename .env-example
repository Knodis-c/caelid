# Postgres
PG_URI=postgresql://caelid:radahn@localhost:5432/caelid_development
PG_TEST_URI=postgresql://caelid:radahn@localhost:5432/caelid_test

# Webpack
WEBPACK_DEV_SERVER_PORT=8080
